    <\/label>\r\n    <\/div>\r\n  <\/div>\r\n\r\n  <p id=\"checkboxError\" style=\"color: red; font-size 10px;\r\n display: none; margin-top: 8px;\">Du m\u00e5 godta vilk\u00e5rene for \u00e5 registrere deg.<\/p>\r\n<\/form>\r\n\r\n<!-- Make sure this div exists and has the correct ID -->\r\n<!-- In Elementor, set the CSS ID of this section to \"sec2\" under Advanced > CSS ID -->\r\n<div id=\"sec2\" style=\"text-align: center;\">\r\n\r\n<!-- Script -->\r\n<script>\r\ndocument.addEventListener(\"DOMContentLoaded\", function () {\r\n  if (typeof jQuery === 'undefined') {\r\n    \/\/ console.log(\"jQuery is not loaded\");\r\n    return;\r\n  }\r\n\r\n  jQuery(document).on('elementor\/popup\/show', function () {\r\n    console.log(\"Elementor popup shown\");\r\n\r\n    const button = document.getElementById(\"myButton\");\r\n    const checkbox = document.getElementById(\"myCheckbox\");\r\n    const error = document.getElementById(\"checkboxError\");\r\n    const sec1 = document.getElementById(\"sec11\");\r\n    const sec2 = document.getElementById(\"sec2\");\r\n\r\n    if (!button || !checkbox || !error || !sec11 || !sec2) {\r\n      console.log(\"Some elements are missing\");\r\n      return;\r\n    }\r\n\r\n    jQuery(sec2).hide(); \/\/ Hide sec2 initially\r\n\r\n    button.addEventListener(\"click\", function () {\r\n      \r\n\r\n      if (checkbox.checked) {\r\n        error.style.display = \"none\";\r\n        sec1.style.display = \"none\";\r\n        jQuery(sec2).fadeIn(); \/\/ Show sec2 with fade effect\r\n        \r\n      } else {\r\n        error.style.display = \"block\";\r\n        sec1.classList.remove(\"hidden\");\r\n        jQuery(sec2).hide();\r\n        \r\n      }\r\n    });\r\n  });\r\n});\r\n<\/script>","_padding":{"unit":"px","top":"0","right":"30","bottom":"30","left":"30","isLinked":false},"_padding_mobile":{"unit":"px","top":"0","right":"0","bottom":"30","left":"0","isLinked":false}},"elements":[],"isInner":false,"widgetType":"html","elType":"wi
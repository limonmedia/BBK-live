{
    "name": "krokedil\/woocommerce",
    "description": "A library package for Krokedils plugins that contains helper methods when working with WooCommerce.",
    "type": "library",
    "version": "1.8.0",
    "require-dev": {
        "php": "~7.4 || ~8.0",
        "php-stubs\/wordpress-stubs": "^6.1",
        "php-stubs\/woocommerce-stubs": "^7.2",
        "10up\/wp_mock": "^1.0"
    },
    "license": "GPL-V2",
    "autoload": {
        "psr-4": {
            "KrokedilAvardaDeps\\Krokedil\\WooCommerce\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KrokedilAvardaDeps\\Krokedil\\WooCommerce\\Tests\\": "tests\/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "authors": [
        {
            "name": "Krokedil AB",
            "email": "info@krokedil.se"
        }
